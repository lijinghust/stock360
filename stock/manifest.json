{
  "manifest_version": 2,
  "name": "stock",
  "version": "1.0.1",

  "description": "This extension demonstrates a browser action with stock.",

  "permissions": ["tabs", "*://*/*" ],
  // "content_security_policy": "script-src 'self' https://qt.gtimg.cn; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19" : "images/logo19.png",
      "38" : "images/logo38.png",
      "48" : "images/logo48.png",
      "128" : "images/logo128.png"
    },
    "default_popup": "index.html"
  },
  "background" : {
    "scripts" : ["background.js"]
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}